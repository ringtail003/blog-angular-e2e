{
  "name": "blog-angular-e2e",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve -c e2e",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:protractor": "ng e2e --no-webdriver-update --protractor-config e2e/protractor/conf.js",
    "e2e:puppeteer": "ng e2e --no-webdriver-update --protractor-config e2e/puppeteer/conf.js",
    "cypress:open": "cypress open --config-file ./e2e/cypress/cypress.json --browser chrome",
    "cypress:run": "cypress run --config-file ./e2e/cypress/cypress.json --browser chrome",
    "e2e:cypress": "start-server-and-test start:e2e http://localhost:4201 cypress:run",
    "e2e:cypress:ide": "start-server-and-test start:e2e http://localhost:4201 cypress:open",
    "prettier": "prettier"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/puppeteer-core": "^2.0.0",
    "angular-in-memory-web-api": "^0.11.0",
    "codelyzer": "^6.0.0",
    "cypress": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.0",
    "protractor": "~7.0.0",
    "puppeteer-core": "^5.5.0",
    "start-server-and-test": "^1.11.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
